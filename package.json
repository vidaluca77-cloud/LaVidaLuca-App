{
  "name": "la-vida-luca",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "dev:api": "cd apps/ia && python main.py",
    "test:api": "cd apps/ia && pytest tests/ -v",
    "install:api": "cd apps/ia && pip install -r requirements.txt",
    "setup": "npm install && npm run install:api"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  }
}
