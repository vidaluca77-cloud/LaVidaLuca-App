{
  "name": "la-vida-luca",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:ia": "cd apps/ia && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "test:ia": "cd apps/ia && python -m pytest tests/ -v",
    "install:ia": "cd apps/ia && pip install -r requirements.txt",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:ia\"",
    "setup": "npm install && npm run install:ia",
    "test": "npm run lint && npm run test:ia"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@heroicons/react": "^2.0.18"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "concurrently": "^8.2.2"
  }
}
